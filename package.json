{
  "name": "vite-project",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "EXTERNAL_API_HOST=$(osascript -e \"IPv4 address of (system info)\") && vite --force --host=$EXTERNAL_API_HOST --port 3004",
    "build": "tsc --declaration && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview --host",
    "test": "vitest --config ./vitest.config.ts",
    "docker-build:prod:local": "docker build -f Dockerfile.local -t mswt-client_prod-local .",
    "docker-build:prod": "docker buildx build --platform linux/amd64 -t mswt-client_prod ."
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@reduxjs/toolkit": "^1.9.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.2.1",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@types/react-redux": "^7.1.25",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "@vitejs/plugin-react-swc": "^3.3.1",
    "antd": "^4.24.10",
    "antd-dayjs-webpack-plugin": "^1.0.6",
    "bson": "^4.7.2",
    "d3": "^7.8.4",
    "dayjs": "^1.11.7",
    "eslint": "^8.41.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "jest": "^29.5.0",
    "jest-css-modules-transform": "^4.4.2",
    "js-cookie": "^3.0.5",
    "nanoid": "^4.0.2",
    "rc-picker": "^3.7.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router": "^6.11.2",
    "react-router-dom": "^6.11.2",
    "sass": "^1.62.1",
    "styled-components": "^6.0.0-rc.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "vite": "^4.3.2",
    "vite-plugin-pwa": "^0.16.3",
    "workbox-precaching": "^7.0.0"
  },
  "devDependencies": {
    "@types/styled-components": "^5.1.26",
    "jsdom": "^24.1.1",
    "less": "^4.1.3",
    "typescript": "^5.0.2",
    "vitest": "^2.0.5"
